#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    vector<int> answer;
    int a,n;
    cin>>n;
    for(;n!=0;--n){
        cin>>a;
        answer.push_back(a);
    }
    for(auto itr=answer.end()-1;itr>=answer.begin();--itr){
        cout<<*itr<<" ";
    }   
    return 0;
}
